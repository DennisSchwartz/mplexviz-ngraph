  "license": "MIT",
  "main": "lib/mplexviz.js",
  "scripts": {
    "test": "echo 'WARNING: no test specified'",
    "build": "mkdirp build && browserify -p tinyify -r ./:mplexviz-ngraph -o build/bundle.js",
    "build-browser": "npm run build",
    "watch": "watchify -r ./:mplexviz-ngraph -v -o build/bundle.js",
    "w": "prunner \"npm run sniper\" \"npm run watch\"",
    "sniper": "sniper ."
  },
  "dependencies": {
    "acorn": "^6.0.4",
    "babyparse": "^0.4.3",
    "backbone": "^1.2.3",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "mplexnet": "0.2.7",
    "multilayerlayout3d": "0.1.3",
    "ngraph.graph": "0.0.12",
    "ngraph.three": "^0.0.14"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "mkdirp": "^0.5.0",
    "prunner": "1.x",
    "sniper": "0.x",
    "tinyify": "^2.4.3",
    "watchify": "^3.11.0"
  },
  "sniper": {
    "buildJS": [
      "https://github.com/DennisSchwartz/mplexviz-ngraph/releases/download/v1.1.0/bundle.js"
    ],
    "buildCSS": [
      "https://cdn.jsdelivr.net/gh/DennisSchwartz/mplexviz-ngraph@1.1.0/style/style.css",
      "https://cdn.jsdelivr.net/gh/DennisSchwartz/mplexviz-ngraph@1.1.0/style/bootstrap.css"
    ],
    "snippets": [
      "examples"
    ],
    "first": "one"
  },
  "style": "style/*.css",
  "keywords": [
    "networks",
    "multilayer",
    "multiplex",
    "complex",
    "network",
    "biojs",
    "biology",
    "big data",
    "visualisation",
    "visualization",
    "ngraph",
    "gmkhsuorlom4n85u"
  ]
